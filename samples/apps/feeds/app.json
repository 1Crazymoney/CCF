{
  "endpoints": {
    "/feeds/{dnsName}": {
      "get": {
        "js_module": "src/endpoints/feeds.js",
        "js_function": "getFeedNamespace",
        "forwarding_required": "never",
        "execute_outside_consensus": "never",
        "authn_policies": [],
        "readonly": true
      },
      "put": {
        "js_module": "src/endpoints/feeds.js",
        "js_function": "setFeedNamespace",
        "forwarding_required": "always",
        "execute_outside_consensus": "never",
        "authn_policies": [],
        "readonly": false
      }
    },
    "/feeds/{dnsName}/{itemName}": {
      "post": {
        "js_module": "src/endpoints/feeds.js",
        "js_function": "recordItem",
        "forwarding_required": "always",
        "execute_outside_consensus": "never",
        "authn_policies": [],
        "readonly": false
      }
    }
  }
}
